{
  "executionRoleArn": "arn:aws:iam::002558080376:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "env-checker",
          "image": "<IMAGE1_NAME>",
          "essential": true,
          "portMappings": [
              {
                  "hostPort": 3000,
                  "protocol": "tcp",
                  "containerPort": 3000
              }
          ],
          "environment": [
            {
              "name": "TEST_ENV",
              "value": "sasa"
            }
          ],
          "logConfiguration": { 
            "logDriver": "awslogs",
            "options": { 
                "awslogs-group" : "/ecs/fargate-task-definition",
              "awslogs-region": "ap-northeast-1",
              "awslogs-stream-prefix": "ecs"
            }
          }
      }
  ],
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "env-checker"
}