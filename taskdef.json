{
  "executionRoleArn": "arn:aws:iam::002558080376:role/ecsTaskExecutionRole",
  "containerDefinitions": [
      {
          "name": "env-checker",
          "image": "<IMAGE1_NAME>",
          "essential": true,
          "portMappings": [
              {
                  "hostPort": 3000,
                  "protocol": "tcp",
                  "containerPort": 3000
              }
          ],
          "enviroment": [
            {
              "name": "TEST_ENV",
              "value": "sasa"
            }
          ]
      }
  ],
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "env-checker"
}